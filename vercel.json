{
  "version": 2,
  "name": "dncl-ebay-manager",
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "client/build",
  "functions": {
    "api/index.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/health",
      "dest": "/api/index.ts"
    },
    {
      "src": "/ebay-auth",
      "dest": "/client/build/index.html"
    },
    {
      "src": "/privacy",
      "dest": "/client/build/index.html"
    },
    {
      "src": "/terms",
      "dest": "/client/build/index.html"
    },
    {
      "src": "/login",
      "dest": "/client/build/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 