{
  "version": 2,
  "name": "dncl-ebay-manager",
  "builds": [
    {
      "src": "server/src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/src/index.ts"
    },
    {
      "src": "/health",
      "dest": "/server/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server/src/index.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
} 